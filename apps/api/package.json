{
  "name": "@wipshare/api",
  "version": "1.0.0",
  "description": "WipShare API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts --env-file=.env.local",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/scripts/seed.ts",
    "seed-tiers": "tsx src/scripts/seed-tiers.ts",
    "regenerate-waveforms": "tsx src/scripts/regenerate-waveforms.ts",
    "dev:sqlite": "cp prisma/schema.dev.prisma prisma/schema.prisma && npm run db:generate && tsx watch src/index.ts",
    "postinstall": "npm run db:generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@clerk/express": "^1.7.21",
    "@google-cloud/logging": "^11.2.0",
    "@google-cloud/monitoring": "^4.1.0",
    "@google-cloud/storage": "^7.16.0",
    "@prisma/client": "^5.19.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.5.4",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "firebase-admin": "^13.4.0",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "resend": "^6.0.1",
    "typescript": "^5.6.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "prisma": "^5.19.1",
    "tsx": "^4.19.0"
  }
}
